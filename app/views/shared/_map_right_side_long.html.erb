<div id="map"></div>
  <script>
  function initMap() {
    var uluru = {
      <%= binding.pry %>
      <% @listings.each do |listing| %>
        lat: <%= @listing.latitude %>, lng: <%= @listing.longitude %>
      <% end %>
    };
    var map = new google.maps.Map(document.getElementById('map'), {
      zoom: 12,
      center: uluru
    });
    var marker = new google.maps.Marker({
      position: uluru,
      map: map
    });
  }
  </script>
  <script src="https://maps.googleapis.com/maps/api/js?key=<%= ENV['Google-API-Key'] %>&callback=initMap"async defer>
</script>
