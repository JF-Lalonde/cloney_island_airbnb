- if @listings
  - @listings.each do |listing|
    .item
      .card-image
        = link_to image_tag(listing.listing_images.first.property_image), listing_path(listing)
      .card-details
        %span.price.badge
          $#{listing.price}
        %span.name
          = listing.name
        %span.room
          = "#{listing.room_type} | #{listing.beds} beds"
      -# .card_review.verticle_card_review
        -# %p= listing.average_rating
