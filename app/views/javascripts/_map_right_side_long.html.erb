<div id="map"></div>
  <script>
  function initMap() {
    var map = new google.maps.Map(document.getElementById('map'), {
      zoom: 11,
      center:{lat: <%= @listings.first.latitude %>,
              lng: <%= @listings.first.longitude %>}
    });

    // var image = listing.listing_images.first.property_image;

    <% @listings.each do |listing| %>
      var marker = new google.maps.Marker({
        position: {lat: <%= listing.latitude %>, lng: <%= listing.longitude %>},
        map: map
        // icon: listing.listing_images.first.property_image
      });
    <% end %>
  }

  </script>
  <script src="https://maps.googleapis.com/maps/api/js?key=<%= ENV['Google-API-Key'] %>&callback=initMap"async defer>
</script>
