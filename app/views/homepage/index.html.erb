<!-- GOOGLE MAP -->
<div class="container">
    <div id="map"></div>

    <script src="https://maps.googleapis.com/maps/api/js?key=<%= ENV['Google-API-Key'] %>&callback=initMap"async defer>
    </script>

    <script>
    function initMap() {
      var uluru = {<%= @listing.latitude %>, <%= @listing.longitude %>};
      var map = new google.maps.Map(document.getElementById('map'), {
        zoom: 4,
        center: uluru
      });
      var marker = new google.maps.Marker({
        position: uluru,
        map: map
      });
    }
    </script>

    <h2><%= link_to "See all >", listings_path %></h2>
    <%=render partial: "shared/vertical_listing_card" %>
  </div>
